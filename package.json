    "@types/react-test-renderer": "16.0.1",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "eslint": "4.19.1",
    "jest": "22.4.3",
    "prettier": "1.12.1",
    "react-native-typescript-transformer": "1.2.5",
    "react-test-renderer": "16.3.2",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.12.0",
    "tslint-eslint-rules": "5.3.1",
