{
  "compilerOptions": {
    "target": "es2015",
    "jsx": "react-native",
    "noEmit": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "importHelpers": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "esModuleInterop": true,
    "types": [
      "react",
      "react-native",
      "jest"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
}